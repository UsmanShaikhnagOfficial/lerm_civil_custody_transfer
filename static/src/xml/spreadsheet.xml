<templates  xml:space="preserve">
    <t t-name="lerm_civil.fill_datasheet" owl="1">
        <div>
            <button type="button" class="btn btn-primary create_spreadsheet" t-on-click="insertInDatasheet()">
                Fill Datasheet
            </button>
        </div>
    </t>
    <t t-name="lerm_civil.set_result" owl="1">
        <div>
            <button type="button" class="btn btn-primary" t-on-click="getDataSheetFromParameter()">
                    Set Result
            </button>
        </div>
    </t>

    <t t-name="lerm_civil.fetch_required_datasheet" owl="1">
        <div>
            <button type="button" class="btn btn-primary" t-on-click="FetchDataSheets()">
                    Fetch Datasheet
            </button>
        </div>
    </t>

    <t t-name="lerm_civil.fill_datasheet_eln" owl="1">
        <div>
            <button type="button" class="btn btn-primary create_spreadsheet" t-on-click="openDatasheet()">
                Fill Datasheet
            </button>
        </div>
    </t>

    <t t-name="lerm_civil.update_result" owl="1">
        <div>
            <button type="button" class="btn btn-primary" t-on-click="updateResult()">
                    Update Result
            </button>
        </div>
    </t>

</templates>
